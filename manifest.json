{
  "manifest_version": 3,
  "name": "Azure Board Glowup",
  "version": "0.0.0.1",
  "description": "A stylesheet to declutter the Azure DevOps Board UI",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "css": ["styles.css"],
      "matches": ["https://dev.azure.com/*"]
    }
  ]
}
